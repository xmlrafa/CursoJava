		civic.Acelerar();
		civic.Acelerar();
		civic.Acelerar();
		civic.Acelerar();
		
		Carro ferrari = new Ferrari(480);
		ferrari.Acelerar();
		ferrari.Acelerar();
		ferrari.Acelerar();
		ferrari.Acelerar();
		ferrari.Acelerar();
		ferrari.Acelerar();
		ferrari.Acelerar();
		ferrari.Acelerar();
		ferrari.Acelerar();
		ferrari.Acelerar();
		ferrari.Acelerar();
		ferrari.Acelerar();
	
		
		System.out.println(ferrari.velocidade);
		System.out.println(civic.velocidade);
		
		
		
		
	}
}
