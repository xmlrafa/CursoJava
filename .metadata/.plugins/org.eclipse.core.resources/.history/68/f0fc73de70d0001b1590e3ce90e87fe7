package oo.heranca.desafio;

public class Ferrari extends Carro {
	
	Ferrari(int velocidadeMaxima){
		super(velocidadeMaxima);
	}
	
	void Acelerar(){
		super.velocidade += 15;
		if (velocidade + delta > VELOCIDADE_MAXIMA) {
			velocidade = VELOCIDADE_MAXIMA;
		}else {		
		velocidade += delta;
		}	}
}
